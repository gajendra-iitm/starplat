function Compute_SSSP (Graph g, node src)
{
        propNode <int> dist;
        g.attachNodeProperty(dist=INF);
        src.dist=0;
        int accum = 0;
        forall (v in g.nodes())
        {
                int count = 0;
                forall (nbr in g.neighbors(v))
                {
                        count = count + nbr.dist;
                }
                accum += count;
        }
}
