function test(Graph g, node src)
{
    propNode<int> prop;
    g.attachNodeProperty(prop =  10);

    int sumV = 0;
    forall(v in g.nodes()){
        sumV = sumV + v.prop;
    }

    while(True)
    {
        for(v in g.nodes()){
            int temp = v.prop;
            temp++;
        }
    }
}