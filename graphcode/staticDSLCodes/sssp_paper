function computeSSSP (Graph g ,node src ) {
    propNode<int> dist ;
    propNode<bool> modified ;
    g.attachNodeProperty(dist = INF , modified = False ) ;
    src.modified = True ;
    src.dist=0;
    bool finished = False ;
    fixedPoint until ( finished : !modified ) {
        forall ( v in g.nodes().filter(modified == True )) {
            
        }
    }
}